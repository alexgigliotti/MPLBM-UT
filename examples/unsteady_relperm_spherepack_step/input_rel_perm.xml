<?xml version="1.0" ?>

<geometry>
    <!-- The name of the .dat file that contains the entire geometry-->
    <file_geom> spheres4Palabos_102_100_100 </file_geom>
    <!-- Size (in voxels) of the simulation domain. -->
    <size> <x> 102 </x> <y> 100 </y> <z> 100 </z> </size>
	<!-- Turn periodicty TRUE or FALSE in x,y,z. -->
	<per> <x> false </x> <y> false </y> <z> false </z> </per>
</geometry>

<folder>
    <!-- Output folder -->
    <out_f> 4relperm/ </out_f>
    <!-- Input folder -->
    <in_f> input/ </in_f>
</folder>

<simulations>
	<!-- Total number of geometries / individual simulations -->
	<num> 9 </num>
	<!-- Pressure-->
    <press> 0.0005 </press>
	<!-- Max iterations-->
    <iter> 25000000 </iter>
	<!-- Convergence-->
    <conv> 0.000001 </conv>
	<!-- Save vtks for medium and velocity-->
    <vtk_out> false </vtk_out>
</simulations>

